/* pages/mine/mine.wxss */
page{
    background: #F7F7F7;
}
.header{
    background-size: 100% 100%;
    background-repeat: no-repeat;
    background-position: center center;
    padding: 35rpx 41rpx 63rpx;
    background-color: #fff;
}
.parentPic{
    width: 148rpx;
    height: 148rpx;
    border-radius: 50%;
    overflow: hidden;
}
.parentPic image{
    display: block;
    width: 100%;
}
.memberInfor{
    position: relative;
}
.memberInfor .box{
    display: flex;
}
.parentTxt{
    margin-left: 44rpx;
}
.userName{
    font-size: 34rpx;
    height: 46rpx;
    color: #fff;
    display: flex;
    align-items: center;
    margin-top:22rpx; 
    margin-bottom: 20rpx;
}
.userName .rz{
    display: flex;
    align-items: center;
    padding: 0 9rpx;
    height: 34rpx;
    background: #fff;
    color: #0F6CDC;
    font-size: 24rpx;
    margin-left: 20rpx;
    border-radius: 4rpx;
}
.userName .rz image{
    width: 24rpx;
    height: 24rpx;
    margin-right: 4rpx;
}
.parentTxt .code{
    font-size: 30rpx;
    line-height: 40rpx;
    color: #fff;
}
.memberInfor .settingBtn{
    position: absolute;
    top: 0;
    right: 11rpx;
    width: 38rpx;
    height: 40rpx;
    overflow: hidden;
}
.memberInfor .settingBtn image{
    display: block;
    width: 100%;
    height: 100%;
}
.fnList{
    background: #fff;
    padding-top: 10rpx;
}
.sameStyle{
    padding: 0 24rpx;
}
.box-same{
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 95rpx;
    font-size: 32rpx;
    color: #333333;
    border-bottom: 1rpx solid #EEEEEE;
}
.box-left{
    display: flex;
    align-items: center;
}
.box-left .icon-fn{
    width: 49rpx;
    height: 44rpx;
    margin-right: 36rpx;
}
.box-same .more{
    width: 16rpx;
    height: 30rpx;
}
.sameStyle .box-same:last-child{
    border-bottom:none;
}
.roughB{
    border-bottom: 16rpx solid #F7F7F7;
}
.box-same .edit{
    font-size: 32rpx;
    color: #c5c5c5;
}

button::after {
    border: none;
}

button {
    background-color: #fff;
    padding:0;
}

.button-hover{
    background: #fff;
}

/*2018-10-26新增样式*/
.userName .myName{
  display: block;
  max-width: 262rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}